

def main()

    maxHt = int(input("Enter Height: "))
    curHt = 0
    out = input("Enter Outline: ")
    fil = input("Enter Fill: ")

    # iterate height
    while curHt < maxHt:
        print(out , end="")

        # print middle
        if curHt > 1 and curHt < maxHt:
            count = 0
            while count < curHt - 1:
                print(fil , end="")

        # end line
        print(out)

        curHt += 1

    # finish triangle
    print(out * maxHt)
    
