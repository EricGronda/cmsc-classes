numDogs = 3
numCats = 6
numFish = 2

print("I have" , numDogs , "dogs as pets.")
print("I have" , numCats , "cats as pets.")
print("I have" , numFish , "fish as pets.")
print("")
print("I have a total of" , numDogs + numCats + numFish , "pets.")
